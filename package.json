        "morgan": "^1.10.0",
        "uuid": "^8.3.2",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.14.6",
        "@babel/node": "^7.14.7",
        "@babel/preset-env": "^7.14.7",
        "eslint": "^7.29.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^3.4.0",
        "nodemon": "^2.0.8",
        "prettier": "^2.3.2"
    },
    "engines": {
        "node": ">=14.1",
        "npm": ">=6.1"
    }
